<package-info xmlns="http://www.simplemachines.org/xml/package-info" xmlns:smf="http://www.simplemachines.org/">
	<id>digger:Halloween_pumpkins</id>
	<name>Halloween Pumpkins Mod</name>
	<version>1.0</version>
	<type>modification</type>
	<install>
		<modification>modification.xml</modification>
	<require-dir name="halloween" destination="$themedir/images" />
	</install>
	<uninstall>
		<modification reverse="true">modification.xml</modification>
		<remove-dir name="$themedir/images/halloween" />
	</uninstall>
</package-info>
